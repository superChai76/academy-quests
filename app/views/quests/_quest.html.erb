<div id="<%= dom_id quest %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= quest.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Is done:</strong>
    <%= quest.is_done? ? "Yes" : "No" %>
  </div>
</div>
